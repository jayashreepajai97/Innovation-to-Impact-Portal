    <httpRuntime maxRequestLength="2147483647" relaxedUrlToFileSystemMapping="true" />
    <compilation debug="true" targetFramework="4.6" />
    <!-- <trust level="Full" /> -->
    <trust level="Full" />
    <pages controlRenderingCompatibilityVersion="4.0" />
    <httpCookies httpOnlyCookies="true" />
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ObligationServiceSoapBinding">
          <security mode="Transport" />
        </binding>
        <binding name="ObligationServiceSoapBinding1" />
      </basicHttpBinding>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IManufacturingInstalledBaseServiceInternal">
          <security mode="Transport">
            <transport clientCredentialType="None" />
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="https://api-uns-sgw.external.hp.com/gw/hpit/egit/obligation.sa/1.1" binding="basicHttpBinding" bindingConfiguration="ObligationServiceSoapBinding" contract="Obligation.Obligation" name="ObligationPort" />
      <endpoint address="https://snr-srv-int.glb.itcs.hpicorp.net/wwsnr/Webservices/ManufacturingInstalledBase/ManufacturingInstalledBaseService.svc" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IManufacturingInstalledBaseServiceInternal" contract="SNRServiceInternal.IManufacturingInstalledBaseService" name="WSHttpBinding_IManufacturingInstalledBaseServiceInternal" />
    </client>
  </system.serviceModel>
  <system.webServer>
    <defaultDocument enabled="true">
      <files>
        <clear />
        <add value="index.html" />
      </files>
    </defaultDocument>
    <modules runAllManagedModulesForAllRequests="true" />
    <!--
        To browse web app root directory during debugging, set the value below to true.
        Set to false before deployment to avoid disclosing web app folder information.
      -->
    <directoryBrowse enabled="false" />
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <httpProtocol>
      <customHeaders>
        <add name="X-Content-Type-Options" value="nosniff" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
